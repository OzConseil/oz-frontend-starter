{
  "name": "oz-frontend-starter",
  "version": "0.1.0",
  "description": "Oz Frontend Starter",
  "repository": {
    "type": "git",
    "url": "git@gitlab.lan:oz/oz-frontend-starter.git"
  },
  "scripts": {
    "clean": "gulp clean",
    "prebuild": "npm run clean && npm install",
    "icons": "gulp iconFont",
    "production": "gulp production",
    "build": "gulp build",
    "prestart": "npm run build",
    "gulp": "gulp",
    "start": "gulp"
  },
  "devDependencies": {
    "babel": "^5.8.12",
    "babel-eslint": "^4.0.5",
    "babelify": "^6.1.3",
    "browser-sync": "^2.8.0",
    "browserify": "^11.0.0",
    "browserify-shim": "^3.8.10",
    "browserify-swap": "^0.2.2",
    "del": "^1.2.0",
    "envify": "^3.4.0",
    "eslint": "^0.24.1",
    "eslint-config-airbnb": "0.0.6",
    "glob": "^5.0.14",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^0.15.0",
    "gulp-filesize": "0.0.6",
    "gulp-iconfont": "^4.0.0",
    "gulp-if": "^1.2.5",
    "gulp-jscs": "^1.6.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.0",
    "gulp-newer": "^0.5.1",
    "gulp-notify": "^2.2.0",
    "gulp-recess": "^1.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-swig": "^0.7.4",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "hbsfy": "^2.2.1",
    "pretty-hrtime": "^1.0.0",
    "require-dir": "^0.3.0",
    "rimraf": "^2.4.2",
    "source-map-support": "^0.3.2",
    "watchify": "^3.3.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "handlebars": "^3.0.3",
    "jquery": "^2.1.4",
    "lodash": "^3.10.0"
  },
  "browserify-shim": {},
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "dist": {
      "underscore.js$": "lodash"
    }
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "hbsfy",
        {
          "extensions": [
            "hbs"
          ]
        }
      ],
      "browserify-swap",
      "browserify-shim",
      "envify"
    ]
  }
}
